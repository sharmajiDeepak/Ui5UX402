{
  "_version": "1.1.0",
  "sap.app": {
	"id": "sap.ux402.ui5",
	"type": "application",
	"i18n": "i18n/i18n.properties",
	"title": "{{appTitle}}",
	"description": "{{appDescription}}",
	"applicationVersion": {
	  "version": "1.0.0"
	}
  },
  "sap.ui": {
	"technology": "UI5",
	"deviceTypes": {
		"desktop": true,
		"tablet": true,
		"phone": true
	}
  },
  "sap.ui5": {
	"rootView": {
		"viewName": "sap.ux402.ui5.view.App",
		"type": "XML",
		"id": "app"
	},
	"dependencies": {
	  "minUI5Version": "1.60",
	  "libs": {
		"sap.m": {}
	  }
	},
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "sap.ux402.ui5.i18n.i18n",
		  "supportedLocales": [""],
		  "fallbackLocale": ""
		}
	  }
    },
    "resources": {
        "css": [
            {
                "uri":"style/style.css"
            }
            
        ]
    },
    "routing": {
        "config":{
            "routerClass": "sap.m.routing.Router",
            "viewType": "XML",
            "viewPath": "sap.ux402.ui5.view",
            "controlId": "app",
            "controlAggregation": "pages",
            "bypassed": {
                "target":["NotFound"]
            }
        },
        "routes":[{
            "pattern": "",
            "name": "Home",
            "target": "Home"
        }, {
            "pattern": "ButtonNav/:greetMesg",
            "name": "ButtonNav",
            "target": "ButtonNav"
        }],
        "targets": {
            "Home": {
                "viewName": "Home",
                "viewId": "Home"
            },
            "ButtonNav": {
                 "viewName": "ButtonNav",
                "viewId": "ButtonNav"
            },
            "NotFound": {
                "viewName": "NotFound"
            }
        }
    }
  }
}